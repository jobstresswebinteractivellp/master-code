<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".userview"
    android:background="@drawable/pet">

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="@string/btn8"
        tools:layout_editor_absoluteX="314dp"
        tools:layout_editor_absoluteY="2dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/button16"
        android:layout_toStartOf="@+id/button16"
        android:text="@string/btn7"
        tools:layout_editor_absoluteX="242dp"
        tools:layout_editor_absoluteY="2dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="77dp"
        android:text="USER DETAILS"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="74dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="@string/btn6"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="518dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="65dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="190dp"
        android:text="userid"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="35dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="159dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView10"
        android:layout_toStartOf="@+id/button18"
        android:layout_toLeftOf="@+id/button18" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="32dp"
        android:text="GO"
        android:layout_alignParentRight="true"
        android:layout_marginRight="32dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="wrap_content"
        android:layout_height="194dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="111dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:text="@string/multext" />
</RelativeLayout>